ul,
li {
  list-style: none;
  margin: 0;
  padding: 0;
}
a {
  text-decoration: none;
  color: #222;
}
@media (min-width: 1440px) {
  .freeUse {
    left: 25.8%;
  }
}
@media (max-width: 1440px) {
  .freeUse {
    left: 20.8%;
  }
}
.topPic {
  height: 400px;
  background: url(../../imgs/market/banner.png) no-repeat 50%;
  background-size: cover;
  position: relative;
}
.topPic .freeUse {
  position: absolute;
  bottom: 20px;
  width: 40%;
  max-width: 600px;
  display: flex;
  border-radius: 3px;
  overflow: hidden;
  line-height: 30px;
  color: #888;
  background-color: #fff;
}
.topPic .freeUse .freeUse_Input {
  width: 100%;
}
.topPic .freeUse .freeUse_Input input {
  width: 90%;
  margin-left: 6%;
  height: 50px;
  border: none;
  font-size: 1em;
  outline: none;
}
.topPic .freeUse .freeUse_Btn {
  width: 120px;
  line-height: 50px;
  text-align: center;
  color: #fff;
  background-color: #433600;
  flex-shrink: 0;
  cursor: pointer;
  user-select: none;
  letter-spacing: 1px;
}
.topPic .freeUse:hover {
  transform: translateY(-2px);
}
.bot {
  background-color: #f7f7f7;
  color: #222;
  min-height: 60vh;
}
.bot .botCt {
  width: 1330px;
  margin: auto;
  padding: 30px 0;
}
.bot .botCt .lable {
  padding: 20px 20px 30px 20px;
  background-color: #fff;
}
.bot .botCt .lable .title {
  font-size: 1.5rem;
  font-weight: bold;
}
.bot .botCt .lable .lableList {
  overflow: hidden;
}
.bot .botCt .lable .lableList li {
  margin-top: 15px;
  margin-right: 15px;
  width: 171px;
  float: left;
  height: 48px;
  line-height: 48px;
  white-space: nowrap;
  text-align: center;
  background-color: #f9f9f9;
  border-radius: 8px;
  overflow: hidden;
  cursor: pointer;
}
.bot .botCt .lable .lableList li:nth-child(7n) {
  margin-right: 0px;
}
.bot .botCt .lable .lableList li.active {
  background-color: #ff6c00;
  color: #fff;
}
.bot .botCt .lable .lableList li:hover {
  transform: translateY(-3px);
}
.bot .botCt .sort {
  margin-top: 30px;
  padding: 20px;
  background-color: #fff;
  display: flex;
  vertical-align: middle;
}
.bot .botCt .sort .sortTitle {
  width: 65px;
  cursor: pointer;
  user-select: none;
  line-height: 33px;
}
.bot .botCt .sort .sortTitle.active {
  color: #ff6c00;
}
.bot .botCt .paperList {
  margin-top: 30px;
  margin-bottom: 0;
  padding: 20px 40px 20px 30px;
  background-color: #fff;
}
.bot .botCt .paperList dd {
  display: flex;
  margin-bottom: 70px;
}
.bot .botCt .paperList dd .left {
  width: 100%;
  margin-right: 150px;
}
.bot .botCt .paperList dd .left .paperTitle {
  font-size: 1.2rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 10px;
}
.bot .botCt .paperList dd .left .paperBrief {
  color: #888;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-bottom: 20px;
  font-size: 0.8rem;
  min-height: 38px;
}
.bot .botCt .paperList dd .left .paperInfo {
  color: #888;
  font-size: 0.8rem;
  display: flex;
}
.bot .botCt .paperList dd .left .paperInfo span {
  padding-right: 50px;
  max-width: 500px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.bot .botCt .paperList dd .right {
  width: 220px;
  flex-shrink: 0;
  padding-top: 15px;
}
.bot .botCt .paperList dd .right .priceCt {
  display: flex;
  background-color: #f9f9f9;
}
.bot .botCt .paperList dd .right .priceCt .price {
  color: #ff9140;
  font-size: 1.8rem;
  width: 120px;
  padding: 10px 15px;
}
.bot .botCt .paperList dd .right .priceCt .price span {
  font-size: 0.9rem;
}
.bot .botCt .paperList dd .right .priceCt .sellInfo {
  width: 100px;
  font-size: 0.8rem;
  color: #666;
  line-height: 22px;
  padding: 9px 0;
}
.bot .botCt .paperList dd .right .btnCt {
  margin-top: 15px;
}
.bot .botCt .pagination {
  background-color: #fff;
  text-align: center;
  padding-bottom: 40px;
}
.bot .botCt .noPaperTips {
  line-height: 40px;
  text-align: center;
  color: #999;
}
.footer_Contain {
  background-color: #dadada;
}
.footer_Contain .aboutUS {
  width: 1330px;
  padding: 20px 0 50px 0;
  margin: auto;
}
.footer_Contain .aboutUS .foot_title {
  font-size: 1.2rem;
}
.footer_Contain .aboutUS .foot_textCodeCt {
  padding: 15px 30px 0px 30px;
  display: flex;
  overflow: hidden;
}
.footer_Contain .aboutUS .foot_textCodeCt .text {
  width: 50%;
  font-size: 0.8rem;
  line-height: 22px;
}
.footer_Contain .aboutUS .foot_textCodeCt .text a {
  color: #4f7bfc;
  margin-right: 15px;
  text-decoration: underline;
}
.footer_Contain .aboutUS .foot_textCodeCt .code {
  width: 50%;
  display: flex;
  justify-content: flex-end;
  text-align: center;
  font-size: 0.8rem;
  padding-top: 10px;
}
.footer_Contain .aboutUS .foot_textCodeCt .code div {
  margin-left: 3vw;
}
.footer_Contain .aboutUS .foot_textCodeCt .code img {
  width: 80px;
  height: 80px;
  margin-bottom: 3px;
}
.ABSBot {
  background-color: #2c2c2c;
  color: #a9adb0;
  padding: 10px;
  text-align: center;
}
.marketC {
  border: 1px solid #ddd;
  border-radius: 3px;
  color: #000;
  width: 1440px;
  margin: auto;
}
.marketC .top_input {
  padding: 10px 30px;
}
.marketC .mindle_select {
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  font-size: 0.8rem;
  display: flex;
}
.marketC .mindle_select .leftOne {
  width: 110px;
  flex-shrink: 0;
  background-color: #f3f2f7;
  color: #000;
  text-align: center;
  font-weight: bold;
  line-height: 40px;
}
.marketC .mindle_select .rightS {
  width: 100%;
  padding: 20px;
}
.marketC .mindle_select .rightS .first_lable {
  overflow: hidden;
}
.marketC .mindle_select .rightS .first_lable li {
  float: left;
  padding: 3px 20px;
  margin-right: 10px;
  cursor: pointer;
}
.marketC .mindle_select .rightS .second_lable {
  border: 1px solid #f8c27f;
}
.marketC .mindle_select .rightS .second_lable .second_select {
  padding: 10px 20px;
  overflow: hidden;
}
.marketC .mindle_select .rightS .second_lable .second_select li {
  cursor: pointer;
  border: 1px solid#ddd;
  padding: 2px 10px;
  margin-right: 10px;
  margin-top: 8px;
  float: left;
}
.marketC .mindle_select .rightS .second_lable .third_lable {
  background-color: #fff9f0;
  padding: 5px 30px;
}
.marketC .mindle_select .rightS .second_lable .third_lable ul {
  overflow: hidden;
}
.marketC .mindle_select .rightS .second_lable .third_lable ul li {
  cursor: pointer;
  float: left;
  padding: 3px 15px;
}
.marketC .mindle_select .rightS .first_lable_active {
  background-color: #fd9607;
  color: #fff;
}
.marketC .mindle_select .rightS .second_lable_active {
  border: 1px solid #f8c27f !important;
  color: #fd9607;
}
.marketC .mindle_select .rightS .third_lable_active {
  color: #fd9607;
}
.marketC .bot_paper .sort_bar {
  border-bottom: 1px solid #ddd;
  height: 35px;
  line-height: 35px;
  display: flex;
  font-size: 0.8rem;
}
.marketC .bot_paper .sort_bar div {
  width: 110px;
  text-align: center;
  border-right: 1px solid #ddd;
  cursor: pointer;
}
.marketC .bot_paper .sort_bar div:active {
  background-color: #f2f2f2;
}
.marketC .bot_paper .sort_bar .sortTitle {
  background-color: #f3f2f7;
  border-right: none !important;
  font-weight: bold;
}
.marketC .bot_paper .paperCt {
  padding: 15px 0px 15px 15px;
  overflow: hidden;
}
.marketC .bot_paper .paperCt .paper {
  cursor: pointer;
  box-shadow: 0 0 3px #ddd;
  width: calc(18.333%);
  margin: 0 15px 15px 0;
  float: left;
  border-radius: 3px;
}
.marketC .bot_paper .paperCt .paper .paper_top {
  padding: 15px;
  border-bottom: 1px solid#f0f0f0;
  position: relative;
}
.marketC .bot_paper .paperCt .paper .paper_top:active {
  background-color: #fcfcfc;
}
.marketC .bot_paper .paperCt .paper .paper_top .paperInfo {
  width: 85%;
  height: 73px;
  overflow: hidden;
}
.marketC .bot_paper .paperCt .paper .paper_top .paperInfo .paperName {
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.marketC .bot_paper .paperCt .paper .paper_top .paperInfo .paperBrief {
  font-size: 0.8em;
  overflow: hidden;
  height: 19px;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 2px 0 10px 0;
}
.marketC .bot_paper .paperCt .paper .paper_top .paperInfo .paperOwner {
  font-size: 0.6em;
  color: #999;
}
.marketC .bot_paper .paperCt .paper .paper_top .paperInfo .paperOwner span {
  margin-right: 15px;
}
.marketC .bot_paper .paperCt .paper .paper_top .testNum {
  position: absolute;
  top: 0px;
  right: 15px;
  height: 24px;
  line-height: 24px;
  padding: 0px 10px;
  background-color: #f6ede0;
  color: #9b7950;
  font-size: 0.6em;
  border-radius: 20px;
}
.marketC .bot_paper .paperCt .paper .paper_bot {
  padding: 10px;
}
.marketC .bot_paper .paperCt .paper .paper_bot span {
  margin-right: 10px;
  color: #fd9607;
  font-size: 0.8em;
}
.marketC .bot_paper .paperCt .paper .paper_bot span img {
  height: 14px;
  margin-top: -4px;
  margin-right: 2px;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right {
  float: right;
  margin-top: -7px;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .prise {
  color: red;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .prise span {
  font-size: 1.4rem;
  font-weight: bold;
  color: red;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .Btn {
  color: #fff;
  padding: 3px 15px;
  border-radius: 20px;
  cursor: pointer;
  user-select: none;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .doTest {
  background-color: #4977fc;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .doTest:active {
  background-color: #6f92fa;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .goBuy {
  background-color: #fd9607;
}
.marketC .bot_paper .paperCt .paper .paper_bot .bot_right .goBuy:active {
  background-color: #fca834;
}
.marketC .bot_paper .paperCt .noPaperTips {
  line-height: 40px;
  text-align: center;
  color: #999;
}
.marketC .bot_paper .pageGation {
  text-align: right;
  overflow: hidden;
  margin: 0 15px 15px 0;
}
